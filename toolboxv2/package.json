{
  "name": "re_simple_toolboxv2",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "tauri": "npm run tauri dev --prefix ./simple-core",
    "tauriB": "npm run tauri build --prefix ./simple-core",
    "tauri-mobil": "npm run tauri android dev --prefix ./simple-core",
    "tauri-mobilB": "npm run tauri android build --prefix ./simple-core",
    "dev": "tb -c FastApi startDev main_dev && cross-env NODE_OPTIONS=development webpack --mode development --watch",
    "devD": "tb -c FastApi edit-api main_D 0.0.0.0 5000 -c DB edit_cli RR -c FastApi startDev main ${DEV_MODULES} -n debug -c",
    "live": "tb -c FastApi start main",
    "liveD": "tb -c FastApi edit-api main 0.0.0.0 5000 -c DB edit_cli RR -c FastApi start main -c",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "watch": "cross-env NODE_ENV=development webpack --mode development --watch"
  },
  "dependencies": {
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.1",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "devDependencies": {
    "compression-webpack-plugin": "^11.1.0",
    "cross-env": "^7.0.3",
    "html-loader": "^5.1.0"
  }
}
