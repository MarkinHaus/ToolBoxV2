{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "simple-core",
  "version": "0.1.0",
  "identifier": "com.simple-core.app",
  "build": {
    "devUrl": "http://localhost:5000/",
    "frontendDist": "https://simplecore.app/"
  },
  "app": {
      "withGlobalTauri": true,
    "windows": [
      {
        "title": "simple-core",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ], "resources": [
      "setup.html",
      "starting.html"
    ],
      "android": {
          "versionCode": 100,
          "minSdkVersion": 28
      }
  }
}
