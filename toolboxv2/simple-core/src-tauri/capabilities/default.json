{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "notification:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "args": true,
          "name": "binaries/tb-worker",
          "sidecar": true
        }
      ]
    },
    "shell:allow-spawn",
    "shell:allow-kill"
  ]
}
