{
  "$schema": "../../web/node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "withGlobalTauri": true,
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devPath": "http://localhost:5000/tauri",
    "distDir": "http://localhost:5000/tauri"
  },
  "package": {
    "productName": "widgets",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
        "shell": {
            "all": true,
            "execute": true,
            "sidecar": true,
            "open": true
        },
        "window": {
            "all": true,
            "create": true,
            "center": true
        }
    },
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "re.simple.toolbox.widget.runner",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 650,
        "resizable": true,
        "title": "widgets",
        "width": 550
      }
    ]
  }
}
