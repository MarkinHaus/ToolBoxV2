<!-- file: tb-lang-pycharm/src/main/resources/fileTypes/TB.xml -->

<filetype binary="false" default_extension="tbx" description="TB Language file" name="TB Language">
    <highlighting>
        <options>
            <option name="LINE_COMMENT" value="#"/>
            <option name="COMMENT_START" value=""/>
            <option name="COMMENT_END" value=""/>
            <option name="HEX_PREFIX" value=""/>
            <option name="NUM_POSTFIXES" value=""/>
            <option name="HAS_BRACES" value="true"/>
            <option name="HAS_BRACKETS" value="true"/>
            <option name="HAS_PARENS" value="true"/>
            <option name="HAS_STRING_ESCAPES" value="true"/>
        </options>
        <keywords keywords="fn;let;mut;if;else;match;loop;while;for;in;return;break;continue;async;await;parallel;true;false;None;Some;Ok;Err" ignore_case="false"/>
        <keywords2 keywords="int;float;bool;string;list;dict;type"/>
        <keywords3 keywords="echo;print"/>
        <keywords4 keywords="@config;@shared"/>
    </highlighting>
</filetype>
